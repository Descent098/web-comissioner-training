{% for page in pages %} <!--Lets you iterate through each page -->

    {% if page[0]["theme"] == "dark" %} 
    <style>
    body{
        background-color: #141414;
    }
    p, h1, h2, h3, h4, h5, h6 {
        color: #f0f0f0;
    }
    </style>
    {% endif %}

    {{ page[1] | safe }} <!--content access -->
{% endfor %}
